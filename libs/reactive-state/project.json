{
  "name": "reactive-state",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/reactive-state/src",
  "prefix": "angular-kit",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/reactive-state/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/reactive-state/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/reactive-state/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/reactive-state/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/reactive-state/**/*.ts",
          "libs/reactive-state/**/*.html",
          "libs/reactive-state/events/**/*.ts",
          "libs/reactive-state/events/**/*.html",
          "libs/reactive-state/on-changes/**/*.ts",
          "libs/reactive-state/on-changes/**/*.html"
        ]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "angular"
      }
    }
  },
  "tags": []
}
