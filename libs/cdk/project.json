{
  "name": "cdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/cdk/src",
  "prefix": "angular-kit",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/cdk/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/cdk/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/cdk/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/cdk/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/cdk/**/*.ts",
          "libs/cdk/**/*.html",
          "libs/cdk/coercing/**/*.ts",
          "libs/cdk/coercing/**/*.html",
          "libs/cdk/types/**/*.ts",
          "libs/cdk/types/**/*.html",
          "libs/cdk/memoize/**/*.ts",
          "libs/cdk/memoize/**/*.html",
          "libs/cdk/pipes/**/*.ts",
          "libs/cdk/pipes/**/*.html",
          "libs/cdk/supported/**/*.ts",
          "libs/cdk/supported/**/*.html",
          "libs/cdk/token/**/*.ts",
          "libs/cdk/token/**/*.html",
          "libs/cdk/lazy/**/*.ts",
          "libs/cdk/lazy/**/*.html"
        ]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "angular"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npm publish dist/libs/cdk"
          }
        ]
      },
      "dependsOn": [
        {
          "target": "lint",
          "projects": "self"
        },
        {
          "target": "test",
          "projects": "self"
        },
        {
          "target": "version",
          "projects": "self"
        },
        {
          "target": "build",
          "projects": "self"
        }
      ]
    }
  },
  "tags": []
}
